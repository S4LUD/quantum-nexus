{
  "name": "quantum-nexus",
  "version": "1.2.50",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint:ci": "node scripts/run-eslint-ci.mjs",
    "typecheck": "npx tsc --noEmit",
    "app:build:ci": "npx expo export --platform web --output-dir .expo-build",
    "server:dev": "node scripts/run-server-script.mjs dev",
    "server:build": "node scripts/run-server-script.mjs build",
    "server:test": "node scripts/run-server-script.mjs test",
    "ci:validate": "npm run lint:ci && npm run typecheck && npm run app:build:ci && npm run server:build && npm run server:test"
  },
  "dependencies": {
    "expo": "~54.0.33",
    "expo-asset": "~12.0.12",
    "expo-audio": "~1.1.1",
    "expo-constants": "~18.0.13",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "i18next": "^25.8.4",
    "lucide-react-native": "^0.563.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  }
}
